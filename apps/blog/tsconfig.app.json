{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "tsBuildInfoFile": "dist/tsconfig.app.tsbuildinfo",
    "jsx": "react-jsx",
    "lib": ["dom"],
    "types": [
      "node",
      "@nx/react/typings/cssmodule.d.ts",
      "@nx/react/typings/image.d.ts"
    ],
    "rootDir": "."
  },
  "exclude": [
    "out-tsc",
    "dist",
    "eslint.config.js",
    "eslint.config.cjs",
    "eslint.config.mjs"
  ],
  "include": [
    "docs/**/*.js",
    "docs/**/*.jsx",
    "docs/**/*.ts",
    "docs/**/*.tsx",
    "docs/**/*.md",
    "docs/**/*.mdx"
  ]
}
