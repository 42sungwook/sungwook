{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "tsBuildInfoFile": "dist/tsconfig.app.tsbuildinfo",
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "types": [],
    "rootDir": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "exclude": [
    "out-tsc",
    "dist",
    "eslint.config.js",
    "eslint.config.cjs",
    "eslint.config.mjs"
  ],
  "include": [
    "docs/**/*.js",
    "docs/**/*.jsx",
    "docs/**/*.ts",
    "docs/**/*.tsx",
    "docs/**/*.md",
    "docs/**/*.mdx",
    "src/**/*.ts",
    "src/**/*.tsx"
  ]
}
